<div class="page-header">
  <h1>Listing team <small>sub</small></h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
	    <th>#</th>
	    <th>Name</th>
	    <th>Description</th>
	    <th>Updated at</th>
    </tr>
  </thead>
	<tbody>
	<% @teams.each do |team| %>
	  <tr>
			<td><%= team.id %></td>
	    <td><%= link_to team.name, team_path(team) %></td>
	    <td><%= team.description %></td>
			<td><%= team.updated_at %></td>
	  </tr>
	<% end %>
	</tbody>
</table>

<%= link_to 'New Team', new_team_path %>
